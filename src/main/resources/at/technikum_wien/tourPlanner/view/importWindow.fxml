<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="394.0" prefWidth="229.0"
            xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="at.technikum_wien.tourPlanner.view.ImportWindowController">
    <children>
        <Label alignment="CENTER" layoutX="31.0" layoutY="14.0" text="Select Tour to Import"
               AnchorPane.leftAnchor="28.0" AnchorPane.rightAnchor="28.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <ListView fx:id="jsonFileList" layoutY="41.0" prefHeight="310.0" prefWidth="229.0"
                  AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="50.0"/>
        <Button fx:id="importButton" layoutX="20.0" layoutY="355.0" mnemonicParsing="false" onAction="#importData"
                text="Import" AnchorPane.bottomAnchor="9.0" AnchorPane.leftAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Button fx:id="cancelButton" layoutX="135.0" layoutY="355.0" mnemonicParsing="false" onAction="#closeWindow"
                text="Cancel" AnchorPane.bottomAnchor="9.0" AnchorPane.rightAnchor="30.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="500.0" minWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="at.technikum_wien.tourPlanner.view.AddLogWindowController">
   <Button fx:id="saveLogButton" layoutX="412.0" layoutY="460.0" mnemonicParsing="false" onAction="#addLog"
           text="Save Log" AnchorPane.rightAnchor="124.0"/>
   <Button fx:id="cancelButton" layoutX="510.0" layoutY="460.0" mnemonicParsing="false" onAction="#closeWindow"
           text="Cancel" AnchorPane.rightAnchor="35.0"/>
   <ChoiceBox fx:id="ratingPicker" layoutX="414.0" layoutY="149.0" prefWidth="150.0" AnchorPane.rightAnchor="36.0"/>
   <ChoiceBox fx:id="tourNamePicker" layoutX="414.0" layoutY="233.0" prefWidth="150.0" AnchorPane.rightAnchor="36.0"/>
   <Label layoutX="58.0" layoutY="237.0" text="Tour Name:" AnchorPane.leftAnchor="35.0"/>
   <TextArea fx:id="commentArea" layoutX="61.0" layoutY="296.0" prefHeight="146.0" prefWidth="503.0"
             AnchorPane.leftAnchor="35.0" AnchorPane.rightAnchor="35.0"/>
   <Label layoutX="256.0" layoutY="270.0" prefHeight="18.0" prefWidth="80.0" text="Comment"
          AnchorPane.leftAnchor="256.0" AnchorPane.rightAnchor="265.0"/>
   <Label layoutX="52.0" layoutY="153.0" text="Rating: (1 lowest - 5 highest)" AnchorPane.leftAnchor="35.0"/>
   <DatePicker fx:id="datePicker" layoutX="414.0" layoutY="106.0" prefHeight="25.0" prefWidth="150.0"
               AnchorPane.rightAnchor="36.0"/>
   <Label fx:id="titleLabel" alignment="CENTER" layoutX="253.0" layoutY="22.0" prefHeight="27.0" prefWidth="93.0"
          text="Add Log" AnchorPane.leftAnchor="250.0" AnchorPane.rightAnchor="250.0">
      <font>
         <Font size="18.0"/>
      </font>
   </Label>
   <Label layoutX="34.0" layoutY="78.0" text="Time"/>
   <Label layoutX="474.0" layoutY="78.0" text="Date" AnchorPane.rightAnchor="100.0"/>
   <Label layoutX="105.0" layoutY="110.0" text="hours"/>
   <Label layoutX="226.0" layoutY="110.0" text="minutes"/>
   <Label layoutX="54.0" layoutY="194.0" text="Difficulty: (1 lowest - 100 highest)" AnchorPane.leftAnchor="35.0"/>
   <Spinner fx:id="hoursSpinner" layoutX="34.0" layoutY="106.0" prefHeight="25.0" prefWidth="64.0"/>
   <Spinner fx:id="minutesSpinner" layoutX="157.0" layoutY="106.0" prefHeight="25.0" prefWidth="64.0"/>
   <Spinner fx:id="difficultySpinner" layoutX="415.0" layoutY="190.0" AnchorPane.rightAnchor="36.0"/>
</AnchorPane>

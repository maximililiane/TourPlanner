<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="394.0" prefWidth="229.0"
            xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="at.technikum_wien.tourPlanner.view.ExportDataWindowController">
   <children>
      <Label layoutX="31.0" layoutY="14.0" text="Select Tour to Export">
         <font>
            <Font size="18.0"/>
         </font>
      </Label>
      <ListView fx:id="tourList" layoutY="41.0" prefHeight="310.0" prefWidth="229.0" AnchorPane.bottomAnchor="50.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0"/>
      <Button fx:id="exportButton" layoutX="20.0" layoutY="355.0" mnemonicParsing="false" onAction="#exportData"
              text="Export" AnchorPane.bottomAnchor="9.0">
         <font>
            <Font size="14.0"/>
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="135.0" layoutY="355.0" mnemonicParsing="false" onAction="#closeWindow"
              text="Cancel" AnchorPane.bottomAnchor="9.0">
         <font>
            <Font size="14.0"/>
         </font>
      </Button>
   </children>
</AnchorPane>
